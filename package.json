    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.1.4",
    "@types/nodemailer": "^6.4.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "guacamole-lite": "^0.7.1",
    "handlebars": "^4.7.7",
    "mysql2": "^2.3.3",
    "nest-csv-parser": "^2.0.4",

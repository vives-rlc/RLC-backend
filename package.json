    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.14",
    "@types/jest": "27.5.0",
    "@types/multer": "^1.4.7",
